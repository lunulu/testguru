<h2>All tests</h2>

<p><%= link_to 'New Test', new_test_path %></p>

<% if Test.all.present? %>
  <table>
    <tr>
      <th>Title</th>
      <th>Level</th>
      <th>Questions</th>
      <th colspan="3">Actions</th>
    </tr>

    <%= render @tests %>
<% else %>
  <p>There is no tests yet.</p>
<% end %>
</table>
